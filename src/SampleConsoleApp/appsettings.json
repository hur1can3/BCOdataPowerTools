{
  "BusinessCentralClient": {
    "BaseUrl": "https://api.businesscentral.dynamics.com",
    "CompanyId": "YOUR_CRONUS_COMPANY_ID_GUID",
    "ApiVersion": "v2.0"
  },
  "Authentication": {
    "BearerToken": "YOUR_OAUTH_BEARER_TOKEN"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "BusinessCentral.OData.Client": "Information",
        "SampleConsoleApp.FullFeatureDemoRunner": "Information"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss "
      }
    }
  }
}
